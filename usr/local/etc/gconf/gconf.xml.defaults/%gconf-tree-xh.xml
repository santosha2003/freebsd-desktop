							</local_schema>
						</entry>
						<entry name="dtmf_mode">
							<local_schema short_desc="DTMF iya-thumela">
							</local_schema>
						</entry>
						<entry name="enable_fast_start">
							<local_schema short_desc="Yenza ukuba isebenze ngo-&apos;Fast Start&apos;">
							</local_schema>
						</entry>
						<entry name="enable_early_h245">
							<local_schema short_desc="Ukwenza ukuba i-H.245 isebenze kwangoko">

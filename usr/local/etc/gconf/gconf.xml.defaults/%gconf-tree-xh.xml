							<local_schema short_desc="I-STUN Seva">
							</local_schema>
						</entry>
					</dir>
					<dir name="sound_events">
						<entry name="busy_tone_sound">
							<local_schema short_desc="Isandi esixakekileyo">
								<longdesc>Isandi esiza kudlalwa ekupheleni konxibelelwano okanye kunxulumano nomntu oxakekileyo, ukuba yenziwe isebenze</longdesc>
							</local_schema>
						</entry>
						<entry name="enable_busy_tone_sound">
							<local_schema short_desc="Dlala isandi sokuxakeka">
								<longdesc>Isandi esikhethiweyo siza kudlalwa ekupheleni konxibelelwano okanye kunxulumano nomntu oxakekileyo, ukuba yenziwe isebenze</longdesc>
							</local_schema>
						</entry>
						<entry name="ring_tone_sound">
							<local_schema>
								<longdesc>Isandi esizodlalwa xa unxulumanisa umntu othile, ukuba yenziwe isebenze</longdesc>
							</local_schema>
						</entry>
						<entry name="enable_ring_tone_sound">
							<local_schema short_desc="Dlala isandi sengxolo">
								<longdesc>Isandi esikhethiweyo siza kudlalwa xa unxulumana nomnye umntu, ukuba yenziwe isebenze</longdesc>
							</local_schema>
						</entry>
						<entry name="enable_incoming_call_sound">
							<local_schema short_desc="Dlala isandi kunxulumano ngefowuni olungenayo">
								<longdesc>Isandi esikhethiweyo siza kudlalwa xa kungena unxulumano, ukuba yenziwe isebenze</longdesc>
							</local_schema>
						</entry>
						<entry name="incoming_call_sound">
							<local_schema short_desc="Iisandi sonxulumano ngefowuni engenayo">
								<longdesc>Isandi esiza kudlalwa xa kungena unxulumano, ukuba yenziwe isebenze</longdesc>
							</local_schema>
						</entry>

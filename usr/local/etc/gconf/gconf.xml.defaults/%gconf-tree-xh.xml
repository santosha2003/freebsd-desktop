<?xml version="1.0"?>
<gconf>
	<dir name="schemas">
		<dir name="apps">
			<dir name="ekiga">
				<dir name="protocols">
					<dir name="call_forwarding">
						<entry name="forward_on_no_answer">
							<local_schema short_desc="Gqithisa unxibelelwano oluya kumququzeleli onikiweyo ukuba akukho mpendulo">
								<longdesc>Ukuba iyasebenze, lonke unxulumano ngeefowuni ezingenayo luza kugqithiselwa kumququzeleli obaluliweyo kwindawo engezansi ukuba awuphenduli unxulumano</longdesc>
							</local_schema>
						</entry>
						<entry name="forward_on_busy">
							<local_schema short_desc="Gqithisa unxibelelwano oluya kumququzeleli onikiweyo ukuba uxakekile">
							</local_schema>
						</entry>
						<entry name="always_forward">
							<local_schema short_desc="Gqithisa rhoqo ugqithisa unxulumano kumququzeleli onikiweyo">
								<longdesc>Ukuba iyasebenze, lonke unxulumano ngefowuni ezingenayo luza kugqithiselwa kumququzeleli obaluliweyo kwindawo engezansi</longdesc>
							</local_schema>
						</entry>
					</dir>
					<dir name="ports">
						<entry name="udp_port_range">
							<local_schema short_desc="Uluhlu lwendawo yonxulumano lwe-UDP">
							</local_schema>
						</entry>
						<entry name="tcp_port_range">
							<local_schema short_desc="Uluhlu lwendawo yonxulumano lwe-TCP">
							</local_schema>
						</entry>
					</dir>

						<longdesc>Nokuba ngaba zidlalwe na izandi zezehlo okanye hayi.</longdesc>
					</local_schema>
				</entry>
				<entry name="click_to_move">
					<local_schema short_desc="Khetha isimbo solawulo">
						<longdesc>Khetha ukuba uwarhuqe na amakhadi okanye ucofe emthonjeni uze ulandelise ngesiphelo.</longdesc>
					</local_schema>
				</entry>
				<entry name="show_toolbar">

							</local_schema>
						</entry>
						<entry name="listen_port">
							<local_schema short_desc="Indawo yonxulumano yokumamela">
							</local_schema>
						</entry>
					</dir>
					<dir name="sip">
						<entry name="dtmf_mode">
							<local_schema short_desc="DTMF iya-thumela">
							</local_schema>

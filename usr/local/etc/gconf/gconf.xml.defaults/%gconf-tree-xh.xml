						<entry name="max_frame_rate">
							<local_schema short_desc="Isantya Se-frame">
							</local_schema>
						</entry>
						<entry name="enable_preview">
							<local_schema short_desc="Ukujonga ividiyo phambi koshicilelo">
								<longdesc>Bonisa imifanekiso yakho ekwisixhobo sekhamera esikrinini</longdesc>
							</local_schema>
						</entry>
						<entry name="format">
							<local_schema short_desc="Ukulungiselela kwe vidiyo">
								<longdesc>Khetha inkqubo elungiselela iikhamera zevidiyo (ayisebenzi kuninzi lweekhamera ze-USB)</longdesc>
							</local_schema>
						</entry>
						<entry name="channel">
							<local_schema short_desc="Isiqhagamshelanisi sevidiyo">
								<longdesc>Inombolo yesiqhagamshelanisi sevidiyo yokusetyenziswa (ukukhetha ikhamera, i-tv okanye abanye ovimba)</longdesc>
							</local_schema>
						</entry>
						<entry name="size">
							<local_schema short_desc="Ubungakanani bevidiyo">
								<longdesc>Khetha ubungakanani bevidiyo esasaziweyo: Encinci (QCIF 176x144) okanye enkulu (CIF 352x288)</longdesc>
							</local_schema>
						</entry>
						<entry name="input_device">
							<local_schema short_desc="Isixhobo sekhompyutha esingenisa ividiyo">
								<longdesc>Khetha isixhobo songeniso kwividiyo emasisetyenziswe. Ukuba kukho impazamo logama usebenzisa esi sixhobo umfanekiso wovavanyo uza kusasazwa.</longdesc>
							</local_schema>
						</entry>

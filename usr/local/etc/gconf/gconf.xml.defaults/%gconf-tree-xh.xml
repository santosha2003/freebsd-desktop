						</entry>
						<entry name="listen_port">
							<local_schema short_desc="Indawo yonxulumano yokumamela">
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="codecs">
					<dir name="audio">
						<entry name="maximum_jitter_buffer">
							<local_schema short_desc="Ubukhulu bobuphaku-phaku besigcini sethutyana">
								<longdesc>Ubukhulu besigcini sethutyana esi siphaku-phaku solwamnkelo lwesandi (in ms)</longdesc>
							</local_schema>
						</entry>
						<entry name="enable_silence_detection">
							<local_schema short_desc="Yenza ukuba ikwazi ukufumanisa inzolo">
							</local_schema>
						</entry>
						<entry name="media_list">
							<local_schema short_desc="Uluhlu lwe Codecs ezinesandi">
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="general">

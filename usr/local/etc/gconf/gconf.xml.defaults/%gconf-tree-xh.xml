							</local_schema>
						</entry>
						<entry name="output_device">
							<local_schema short_desc="Isixhobo sekhompyutha enesandi esiphumayo">
								<longdesc>Khetha isixhobo sekhompyutha enesandi esiphumayo esiza kusebenza</longdesc>
							</local_schema>
						</entry>

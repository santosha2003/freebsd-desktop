							<local_schema short_desc="Bunyi nada sibuk">
							</local_schema>
						</entry>
						<entry name="enable_busy_tone_sound">
							<local_schema short_desc="Main nada sibuk">
							</local_schema>
						</entry>
						<entry name="enable_ring_tone_sound">
							<local_schema short_desc="Main nada dering">
							</local_schema>
						</entry>
						<entry name="enable_incoming_call_sound">
							<local_schema short_desc="Mainkan bunyi pada panggilan masuk">
							</local_schema>
						</entry>

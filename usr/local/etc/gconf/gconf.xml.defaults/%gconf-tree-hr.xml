							</entry>
							<entry name="position">
								<local_schema short_desc="Položaj prozora adresara">
									<longdesc>Položaj prozora adresara</longdesc>
								</local_schema>
							</entry>
						</dir>
						<dir name="assistant">
							<entry name="position">
								<local_schema short_desc="Položaj na zaslonu čarobnjakovog prozora">
									<longdesc>Položaj na zaslonu čarobnjakovog prozora</longdesc>
								</local_schema>
							</entry>
							<entry name="size">
								<local_schema short_desc="Veličina čarobnjakovog prozora">
									<longdesc>Veličina čarobnjakovog prozora</longdesc>
								</local_schema>
							</entry>
						</dir>
						<dir name="chat_window">
							<entry name="position">
								<local_schema short_desc="Položaj prozora za razgovor na zaslonu">
									<longdesc>Položaj prozora za razgovor na zaslonu</longdesc>
								</local_schema>
							</entry>
							<entry name="size">
								<local_schema short_desc="Veličina prozora za razgovor">
									<longdesc>Veličina prozora za razgovor</longdesc>

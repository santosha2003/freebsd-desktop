									<longdesc>Položaj prozora s postavkama</longdesc>
								</local_schema>
							</entry>
						</dir>
						<dir name="preferences_window">
							<entry name="size">
								<local_schema short_desc="Veličina prozora postavki">
									<longdesc>Veličina prozora postavki</longdesc>
								</local_schema>

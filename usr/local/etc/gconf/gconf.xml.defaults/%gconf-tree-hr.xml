					</dir>
					<dir name="nat">
						<entry name="stun_server">
							<local_schema short_desc="STUN poslužitelj">
							</local_schema>
						</entry>
					</dir>
					<dir name="sound_events">
						<entry name="busy_tone_sound">
							<local_schema short_desc="Zvuk zauzetog signala">
								<longdesc>Ako je uključen, zvuk će se čuti na kraju poziva kada pozivate zauzetog korisnika.</longdesc>
							</local_schema>
						</entry>
						<entry name="enable_busy_tone_sound">
							<local_schema short_desc="Reproduciraj ton zauzeća">
								<longdesc>Ako je uključeno, odabrani zvuk biti će sviran na kraju razgovora ili pri pozivu nekoga tko je zauzet, ako je omogućeno.</longdesc>
							</local_schema>
						</entry>
						<entry name="ring_tone_sound">
							<local_schema short_desc="Zvuk tona za biranje">
								<longdesc>Ako je uključen, zvuk će se javiti prilikom poziva.</longdesc>
							</local_schema>
						</entry>
						<entry name="enable_ring_tone_sound">
							<local_schema short_desc="Reproducitaj zvuk zvonjenja">
								<longdesc>Ako je uključeno, odabrani zvuk biti će sviran pri odlaznim pozivima.</longdesc>
							</local_schema>
						</entry>
						<entry name="enable_new_voicemail_sound">
							<local_schema short_desc="Reproduciraj zvuk kad stigne nova poruka govorne pošte">
								<longdesc>Ako je omogućeno, odabrani zvuk će biti reproduciran nakon dolaska nove govorne pošte</longdesc>
							</local_schema>
						</entry>
						<entry name="new_voicemail_sound">
							<local_schema short_desc="Zvuk nove zvučne pošte">
								<longdesc>Ako je omogućeno, zvuk koji će se reproducirati kada imate novu govornu poštu</longdesc>
							</local_schema>
						</entry>
						<entry name="enable_new_message_sound">
							<local_schema short_desc="Reproduciraj zvuk kad stigne poruka">
								<longdesc>Ako je omogućeno, odabrani zvuk će biti reproduciran nakon dolaska nove kratke poruke</longdesc>
							</local_schema>
						</entry>
						<entry name="new_message_sound">
							<local_schema short_desc="Zvuk nove kratke poruke">
								<longdesc>Ako je omogućeno, zvuk koji će se reproducirati kada imate novu kratku poruku</longdesc>
							</local_schema>
						</entry>
						<entry name="enable_incoming_call_sound">
							<local_schema short_desc="Reproduciraj zvuk na dolazećim pozivima">
								<longdesc>Ako je uključeno, odabrani zvuk biti će sviran pri dolaznim pozivima.</longdesc>
							</local_schema>
						</entry>
						<entry name="incoming_call_sound">
							<local_schema short_desc="Zvuk dolaznog poziva">
								<longdesc>Ako je uključen, zvuk će se javiti u trenutku dolaznog poziva.</longdesc>
							</local_schema>
						</entry>
						<entry name="output_device">
							<local_schema short_desc="Alternativni audio izlaz">
								<longdesc>Odaberite alternativni audio izlaz za zvučne događaje.</longdesc>
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="devices">
					<dir name="video">
						<entry name="max_frame_rate">
							<local_schema short_desc="slika u sekundi">
							</local_schema>
						</entry>
						<entry name="enable_preview">
							<local_schema short_desc="Pregled videa">
								<longdesc>Prikaži slike sa vaše kamere.</longdesc>
							</local_schema>
						</entry>
						<entry name="format">
							<local_schema short_desc="Video oblik">
								<longdesc>Označite oblik video kamere (nije primjenjivo na većini USB kamera).</longdesc>
							</local_schema>
						</entry>
						<entry name="channel">
							<local_schema short_desc="Video kanal">
								<longdesc>Broj video kanala za uporabu (za odabir kamere, tv-a ili drugih izvora).</longdesc>
							</local_schema>
						</entry>
						<entry name="size">
							<local_schema short_desc="Veličina videa">
								<longdesc>Odaberite veličinu videa koji se prenosi:  Mala (QCIF 176x144) ili velika (CIF352x288).</longdesc>
							</local_schema>
						</entry>
						<entry name="input_device">
							<local_schema short_desc="Ulazni video uređaj">
								<longdesc>Odaberite ulazni video uređaj kojeg želite koristiti. U slučaju prijave pogreške prilikom korištenja uređaja bit će odaslana probna slika.</longdesc>
							</local_schema>
						</entry>
					</dir>
					<dir name="audio">
						<entry name="input_device">
							<local_schema short_desc="Audio dolazeći uređaj">
								<longdesc>Označite ulazni zvučni uređaj kojeg želite koristiti.</longdesc>
							</local_schema>
						</entry>
						<entry name="output_device">
							<local_schema short_desc="Audio izlazni uređaj">
								<longdesc>Označite izlazni zvučni uređaj kojeg želite koristiti.</longdesc>
							</local_schema>
						</entry>
					</dir>
				</dir>
			</dir>
			<dir name="aisleriot">
				<entry name="recent_games_list">
					<local_schema short_desc="Nedavno odigrane igre">
						<longdesc>Popis nedavno odigranih igara.</longdesc>
					</local_schema>
				</entry>
				<entry name="statistics">
					<local_schema short_desc="Statiskita odigranih igara">
						<longdesc>Popis znakova koji dolaze u obliku peterostrukosti: naziv, pobjede, ukupno odigranih igara, najbolje vrijeme (u sekundama) i najlošije vrijeme (isto u sekundama). Neodigrane igre ne trebaju biti prikazane.</longdesc>
					</local_schema>
				</entry>
				<entry name="game_file">
					<local_schema short_desc="Datoteka igre koja će se koristiti">
						<longdesc>Naziv datoteke sheme koja sadrži pasijans koji će se igrati.</longdesc>
					</local_schema>
				</entry>
				<entry name="animations">
					<local_schema short_desc="Animacije">
						<longdesc>Treba li ili ne animirati pokrete karata.</longdesc>
					</local_schema>
				</entry>
				<entry name="sound">
					<local_schema short_desc="Zvuk">
						<longdesc>Treba li ili ne reproducirati zvukove događaja.</longdesc>
					</local_schema>
				</entry>
				<entry name="click_to_move">
					<local_schema short_desc="Odaberi način upravljanja">
						<longdesc>Odaberite hoćete li povlačiti karte ili kliknuti na polazište, pa na odredište.</longdesc>
					</local_schema>
				</entry>
				<entry name="show_statusbar">
					<local_schema short_desc="Treba li ili ne prikazati statusnu traku">
					</local_schema>
				</entry>
				<entry name="show_toolbar">
					<local_schema short_desc="Treba li ili ne prikazati alatnu traku">
					</local_schema>
				</entry>
				<entry name="card_style">
					<local_schema short_desc="Naziv datoteke teme">
						<longdesc>Naziv datoteke koja sadrži grafiku karata.</longdesc>
					</local_schema>
				</entry>

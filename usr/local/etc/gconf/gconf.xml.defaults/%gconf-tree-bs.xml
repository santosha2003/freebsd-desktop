<?xml version="1.0"?>
<gconf>
	<dir name="schemas">
		<dir name="apps">
			<dir name="ekiga">
				<dir name="protocols">
					<dir name="call_forwarding">
						<entry name="forward_on_no_answer">
							<local_schema short_desc="Proslijedi pozive danom računaru ako nema odgovora">
								<longdesc>Ukoliko je uključeno, svi dolazeći pozivi ne koje nećete odgovoriti biti će prosljeđeni na računar koji je određen u dole navedenom polju.</longdesc>
							</local_schema>
						</entry>
						<entry name="forward_on_busy">
							<local_schema short_desc="Proslijedi pozive prema danom računaru ako je linija zauzeta">
							</local_schema>
						</entry>
						<entry name="always_forward">
							<local_schema short_desc="Uvijek preusmjeri pozive na zadani server">
								<longdesc>Ako je uključeno, svi dolazeći pozivi će biti prosljeđeni računaru određenom u dole navedenom polju.</longdesc>
							</local_schema>
						</entry>
					</dir>
					<dir name="ports">
						<entry name="udp_port_range">
							<local_schema short_desc="Raspon UDP porta">
							</local_schema>
						</entry>
						<entry name="tcp_port_range">
							<local_schema short_desc="Raspon TCP porta">
							</local_schema>
						</entry>
					</dir>
					<dir name="h323">
						<entry name="forward_host">
							<local_schema short_desc="Proslijedi pozive prema računaru">
							</local_schema>
						</entry>
						<entry name="dtmf_mode">
							<local_schema short_desc="DTMF slanje">
							</local_schema>
						</entry>
						<entry name="enable_fast_start">
							<local_schema short_desc="Omogući &apos;Fast Start&apos;">
							</local_schema>
						</entry>
						<entry name="enable_early_h245">
							<local_schema short_desc="Omogući rani H.245">
								<longdesc>Ovo omogućava H.245 u ranoj fazi postavljanja.</longdesc>
							</local_schema>
						</entry>
						<entry name="listen_port">
							<local_schema short_desc="Port za slušanje">
							</local_schema>
						</entry>
					</dir>
					<dir name="sip">
						<entry name="dtmf_mode">
							<local_schema short_desc="DTMF slanje">
							</local_schema>
						</entry>
						<entry name="forward_host">
							<local_schema short_desc="Proslijedi pozive prema računaru">
							</local_schema>
						</entry>
						<entry name="listen_port">
							<local_schema short_desc="Port za slušanje">
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="codecs">
					<dir name="audio">
						<entry name="maximum_jitter_buffer">
							<local_schema short_desc="Maksimalna pomoćna memorija za jitter ">
								<longdesc>Veličina najvećeg jitter spremika za prihvat zvuka (u ms).</longdesc>
							</local_schema>
						</entry>
						<entry name="enable_silence_detection">
							<local_schema short_desc="Omogući otkrivanje tišine">
							</local_schema>
						</entry>
						<entry name="media_list">
							<local_schema short_desc="Lista zvučnih kodeka">
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="general">
					<dir name="call_options">
						<entry name="no_answer_timeout">
							<local_schema short_desc="Nema vremenskog ograničenja za odgovor">
								<longdesc>Automatski odbacuje ili prosljeđuje dolazne pozive, ukoliko nema  odgovora nakon određenog vremena (u sekundama). </longdesc>
							</local_schema>
						</entry>
					</dir>
					<dir name="user_interface">
						<dir name="remote_video_window">
							<entry name="position">
								<local_schema short_desc="Pozicija video prozora druge strane">
									<longdesc>Položaj udaljenog prozora za video</longdesc>
								</local_schema>
							</entry>
							<entry name="size">
								<local_schema short_desc="Veličina prozora udaljenog videa">
									<longdesc>Veličina prozora videa druge strane</longdesc>
								</local_schema>
							</entry>
						</dir>
						<dir name="video_display">
							<entry name="video_view">
								<local_schema short_desc="Uobičajni pogled za video">
								</local_schema>
							</entry>
							<entry name="stay_on_top">
								<local_schema short_desc="Postavite prozor videa iznad ostalih prozora tokom poziva.">
									<longdesc>Postavite prozor videa iznad ostalih prozora tokom poziva.</longdesc>
								</local_schema>
							</entry>
							<entry name="zoom">
								<local_schema short_desc="Veličina povećanja">
								</local_schema>
							</entry>
						</dir>
						<dir name="accounts_window">
							<entry name="size">
								<local_schema short_desc="Veličina prozora osobina">
									<longdesc>Veličina prozora osobina</longdesc>
								</local_schema>
							</entry>
							<entry name="position">
								<local_schema short_desc="Položaj prozora s osobinama">
									<longdesc>Položaj prozora s osobinama</longdesc>
								</local_schema>
							</entry>
						</dir>
						<dir name="preferences_window">
							<entry name="size">
								<local_schema short_desc="Veličina prozora osobina">
									<longdesc>Veličina prozora osobina</longdesc>
								</local_schema>
							</entry>
							<entry name="position">
								<local_schema short_desc="Položaj prozora s osobinama">
									<longdesc>Položaj prozora s osobinama</longdesc>
								</local_schema>
							</entry>
						</dir>
						<dir name="addressbook_window">
							<entry name="size">
								<local_schema short_desc="Veličina prozora adresara">
									<longdesc>Veličina prozora adresara</longdesc>
								</local_schema>
							</entry>
							<entry name="position">
								<local_schema short_desc="Položaj prozora adresara">
									<longdesc>Položaj prozora adresara</longdesc>
								</local_schema>
							</entry>
						</dir>
						<dir name="assistant">
							<entry name="position">
								<local_schema short_desc="Položaj na prikazu čarobnjakovog prozora">
									<longdesc>Položaj na prikazu čarobnjakovog prozora</longdesc>
								</local_schema>
							</entry>
							<entry name="size">
								<local_schema short_desc="Veličina čarobnjakovog prozora ">
									<longdesc>Veličina čarobnjakovog prozora </longdesc>
								</local_schema>
							</entry>
						</dir>
						<dir name="main_window">
							<entry name="position">
								<local_schema short_desc="Položaj glavnog prozora">
									<longdesc>Položaj glavnog prozora</longdesc>
								</local_schema>
							</entry>
						</dir>
					</dir>
					<dir name="sound_events">
						<entry name="busy_tone_sound">
							<local_schema short_desc="Zvuk zauzetog signala">
								<longdesc>Ako je uključen, zvuk će se čuti na kraju poziva kada pozivate zauzetog korisnika.</longdesc>
							</local_schema>
						</entry>
						<entry name="enable_busy_tone_sound">
							<local_schema short_desc="Reproduciraj ton zauzeća">
								<longdesc>Ako je uključeno, odabrani zvuk bit će sviran na kraju razgovora ili pri pozivu nekoga ko je zauzet, ako je omogućeno.</longdesc>
							</local_schema>
						</entry>
						<entry name="ring_tone_sound">
							<local_schema>
								<longdesc>Ako je uključen, zvuk će se javiti prilikom poziva.</longdesc>
							</local_schema>
						</entry>
						<entry name="enable_ring_tone_sound">
							<local_schema short_desc="Reproduciraj zvuk zvonjenja">
								<longdesc>Ako je uključeno, odabrani zvuk bit će sviran pri odlaznim pozivima.</longdesc>
							</local_schema>
						</entry>
						<entry name="enable_incoming_call_sound">
							<local_schema short_desc="Reproduciraj zvuk na dolazećim pozivima">
								<longdesc>Ako je uključeno, odabrani zvuk biti će sviran pri dolaznim pozivima.</longdesc>
							</local_schema>
						</entry>
						<entry name="incoming_call_sound">
							<local_schema short_desc="Zvuk dolaznog poziva">
								<longdesc>Ako je uključen, zvuk će se javiti u trenutku dolaznog poziva.</longdesc>
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="devices">
					<dir name="video">
						<entry name="enable_preview">
							<local_schema short_desc="Pregled videa">
								<longdesc>Prikaži slike sa vaše kamere.</longdesc>
							</local_schema>
						</entry>
						<entry name="format">
							<local_schema short_desc="Video oblik">
								<longdesc>Označite oblik video kamere (nije primjenjivo na većini USB kamera).</longdesc>
							</local_schema>
						</entry>
						<entry name="channel">
							<local_schema short_desc="Video kanal">
								<longdesc>Broj video kanala za upotrebu (za odabir kamere, tv-a ili drugih izvora).</longdesc>
							</local_schema>
						</entry>
						<entry name="size">
							<local_schema short_desc="Veličina videa">
								<longdesc>Odaberite veličinu videa koji se prenosi:  Mala (QCIF 176x144) ili velika (CIF352x288).</longdesc>
							</local_schema>
						</entry>
						<entry name="input_device">
							<local_schema short_desc="Ulazni video uređaj">
								<longdesc>Odaberite ulazni video uređaj kojeg želite koristiti. U slučaju prijave greške prilikom korištenja uređaja bit će odaslana probna slika. </longdesc>
							</local_schema>
						</entry>
					</dir>
					<dir name="audio">
						<entry name="input_device">
							<local_schema short_desc="Audio dolazeći uređaj">
								<longdesc>Označite ulazni zvučni uređaj kojeg želite koristiti.</longdesc>
							</local_schema>
						</entry>
						<entry name="output_device">
							<local_schema short_desc="Audio izlazni uređaj">
								<longdesc>Označite izlazni zvučni uređaj kojeg želite koristiti.</longdesc>
							</local_schema>
						</entry>
					</dir>
				</dir>
			</dir>
			<dir name="gconf-editor">
				<entry name="bookmarks">
					<local_schema short_desc="Zabilješke">
						<longdesc>gconf-editor direktorij zabilješki</longdesc>
					</local_schema>
				</entry>
			</dir>
			<dir name="aisleriot">
				<entry name="recent_games_list">
					<local_schema short_desc="Nedavno igrane igre">
						<longdesc>Lista nedavno odigranih igara.</longdesc>
					</local_schema>
				</entry>
				<entry name="statistics">
					<local_schema short_desc="Statistika odigranih igara">
						<longdesc>Lista stringova koji dolaze u formi: ime, pobjede, ukupno odigrane igre, najbolje vrijeme (u sekundama) i najgore vrijeme (u sekundama). Neodigrane igre ne moraju biti zastupljene.</longdesc>
					</local_schema>
				</entry>
				<entry name="game_file">
					<local_schema short_desc="Datoteka igre koja će se koristiti">
						<longdesc>Ime datoteke koja sadrži opis vrste pasijansa koji će se igrati.</longdesc>
					</local_schema>
				</entry>
				<entry name="animations">
					<local_schema short_desc="Animacije">
						<longdesc>Prikazati poteze karatai animacijom ili ne.</longdesc>
					</local_schema>
				</entry>
				<entry name="sound">
					<local_schema short_desc="Zvuk">
						<longdesc>Da li puštati zvukove pri događajima.</longdesc>
					</local_schema>
				</entry>
				<entry name="click_to_move">
					<local_schema short_desc="Izaberite stil upravljanja">
						<longdesc>Izaberite da li da prevlačite karte ili da kliknete na izvornu pa na odredišnu.</longdesc>
					</local_schema>
				</entry>
				<entry name="show_statusbar">
					<local_schema short_desc="Prikazati statusnu traku ili ne.">
					</local_schema>
				</entry>
				<entry name="show_toolbar">
					<local_schema short_desc="Da li prikazivati paletu alata">
					</local_schema>
				</entry>
				<entry name="card_style">
					<local_schema short_desc="Ime datoteke sa temom">
						<longdesc>Ime datoteke sa slikama za karte.</longdesc>
					</local_schema>
				</entry>
			</dir>
		</dir>
	</dir>
</gconf>
